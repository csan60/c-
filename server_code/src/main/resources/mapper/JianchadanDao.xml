<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cl.dao.JianchadanDao">

    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.cl.entity.JianchadanEntity" id="jianchadanMap">
        <result property="xiangmumingcheng" column="xiangmumingcheng"/>
        <result property="jianchaleixing" column="jianchaleixing"/>
        <result property="jianchajiage" column="jianchajiage"/>
        <result property="yishengzhanghao" column="yishengzhanghao"/>
        <result property="yishengxingming" column="yishengxingming"/>
        <result property="huanzhezhanghao" column="huanzhezhanghao"/>
        <result property="huanzhexingming" column="huanzhexingming"/>
        <result property="ispay" column="ispay"/>
    </resultMap>

    <select id="selectListView"
            resultType="com.cl.entity.view.JianchadanView">

        SELECT jianchadan.* FROM jianchadan jianchadan
        <where>1=1 ${ew.sqlSegment}</where>
    </select>

    <select id="selectView"
            resultType="com.cl.entity.view.JianchadanView">
        SELECT * FROM jianchadan jianchadan
        <where>1=1 ${ew.sqlSegment}</where>
    </select>


</mapper>
