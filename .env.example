# Backend API Configuration
# 后端API配置
VUE_APP_BASE_API=http://localhost:8080/daxueshengxinlijiankangpingce/

# Environment Mode (development, production, testing)
# 环境模式
NODE_ENV=development

# Application Name
# 应用名称
VUE_APP_NAME=College Student Mental Health Assessment System

# API Timeout (milliseconds)
# API超时时间（毫秒）
VUE_APP_TIMEOUT=30000

# Whether to enable Mock Data
# 是否启用模拟数据
VUE_APP_ENABLE_MOCK=false

# Log Level (none, error, warn, info, debug)
# 日志级别
VUE_APP_LOG_LEVEL=debug
